{
    "editor.fontSize": 12,
    "editor.fontFamily": "'CaskaydiaCove Nerd Font Mono', 'monospace', monospace",
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.comments.insertSpace": true,
    "editor.detectIndentation": true,
    "editor.lineHeight": 0,
    "editor.minimap.enabled": false,
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.fontFamily": "'CaskaydiaCove Nerd Font Mono', 'monospace', monospace",
    "terminal.integrated.lineHeight": 1,
    "terminal.integrated.shellIntegration.enabled": true,
    "files.trimTrailingWhitespace": true,
    "markdown.preview.fontSize": 10,
    "markdown.preview.lineHeight": 1.0,
    "markdown-preview-github-styles.colorTheme": "light",
    "markdownShiki.theme": "min-light",
    "json.maxItemsComputed": 100000,
    "cSpell.enabled": true,
    "cSpell.caseSensitive": true,
    "cSpell.words": []
}
